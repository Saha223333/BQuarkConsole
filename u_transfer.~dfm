  object ADOMonitorResults: TADOQuery
    Connection = InStarConn
    CursorType = ctStatic
    Parameters = <
      item
        Name = 'ser_num'
        DataType = ftString
        NumericScale = 255
        Precision = 255
        Size = -1
        Value = Null
      end>
    SQL.Strings = (
      
        '--select q2.id,q2.d_value_energy_0_ts_ap as max_pok,q2.date_time' +
        '_energy_0_ts_ap as max_date,q4.address,q2.concentrator_plc_count' +
        'er_id,q4.serial_number from '
      '--instar_skek.dbo.object_concentrator_plc_counter_points as q4,'
      '--instar_skek.dbo.result_monitor_concentrator_plc_results  as q2'
      ''
      '--right join'
      ''
      '--(select max(id) as q1,concentrator_plc_counter_id as q3 from '
      '--instar_skek.dbo.result_monitor_concentrator_plc_results '
      '--group by concentrator_plc_counter_id) ppp on ppp.q1=q2.id'
      ''
      
        '--where q4.id=q2.concentrator_plc_counter_id and q4.serial_numbe' +
        'r=:ser_num'
      ''
      
        'SELECT     a.d_value_energy_0_ts_ap AS max_pok, a2.address AS ad' +
        'ress, a2.ser_num, b_1.md, 0 AS is_can'
      
        'FROM         dbo.result_monitor_concentrator_plc_results AS a RI' +
        'GHT OUTER JOIN'
      
        '                          (SELECT     concentrator_plc_counter_i' +
        'd, MAX(date_time_energy_0_ts_ap) AS md, MAX(id) AS id1'
      
        '                            FROM          dbo.result_monitor_con' +
        'centrator_plc_results'
      
        '                            GROUP BY concentrator_plc_counter_id' +
        ') AS b_1 ON a.concentrator_plc_counter_id = b_1.concentrator_plc' +
        '_counter_id AND '
      
        '                      a.date_time_energy_0_ts_ap = b_1.md AND a.' +
        'id = b_1.id1 RIGHT OUTER JOIN'
      
        '                          (SELECT     serial_number AS ser_num, ' +
        'id, address'
      
        '                            FROM          dbo.object_concentrato' +
        'r_plc_counter_points AS a1) AS a2 ON a2.id = a.concentrator_plc_' +
        'counter_id'
      'WHERE     (a.id IS NOT NULL) and a2.ser_num=:ser_num')
    Left = 200
    Top = 16
  end
